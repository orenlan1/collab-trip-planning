{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "tsc: build - server/tsconfig.json",
      "command": "npx",
      "args": [
        "tsc",
        "-p",
        "server/tsconfig.json"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "silent",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": ["$tsc"]
    },
  ]
}